{
	"name": "@suckless/jsx",
	"version": "0.1.0",
	"description": "JSX-to-string runtime for server-side HTML rendering with XSS escaping and strict per-element types",
	"keywords": [
		"escape",
		"html",
		"jsx",
		"ssr",
		"suckless",
		"template",
		"typescript"
	],
	"homepage": "https://github.com/brielov/suckless/tree/master/packages/jsx#readme",
	"bugs": "https://github.com/brielov/suckless/issues",
	"license": "MIT",
	"author": "brielov",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brielov/suckless.git",
		"directory": "packages/jsx"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"sideEffects": false,
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./jsx-runtime": {
			"bun": "./src/jsx-runtime.ts",
			"import": {
				"types": "./dist/jsx-runtime.d.ts",
				"default": "./dist/jsx-runtime.js"
			}
		},
		"./jsx-dev-runtime": {
			"bun": "./src/jsx-dev-runtime.ts",
			"import": {
				"types": "./dist/jsx-dev-runtime.d.ts",
				"default": "./dist/jsx-dev-runtime.js"
			}
		},
		"./package.json": "./package.json"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	}
}
